{
    "name": "cucumber-webdriverio",
    "version": "0.1.0",
    "main": "wdio.conf.js",
    "keywords": [],
    "author": "Yon Cuadrado <yon.cuadrado@gmail.com>",
    "license": "ISC",
    "description": "Example of testing project that includes cucumber-js, page object model, webdriverio and allure",
    "scripts": {
        "test:local:chrome": "wdio ./test/cucumber/configuration/js/local-conf.js --browser=chrome --maxInstances=1",
        "test:local:chrome:1Test": "TS_NODE_FILES=true TS_NODE_TRANSPILE_ONLY=true TS_NODE_BUILD=true TS_NODE_PROJECT=tsconfig.json TS_NODE_LOG_ERROR=true wdio run ./test/cucumber/configuration/js/local-conf.js --spec \"test/cucumber/features/ClickOnCheckboxes.feature\" --browser=chrome --maxInstances=1",
        "test:generate:steps": "wdio ./test/cucumber/configuration/js/local-conf.js --generateSteps --browser=chrome",
        "getStepsSnippetsWithCucumber": "./node_modules/.bin/cucumber-js --require ./dist/cucumber/pages/**/*.js --format 'snippets' --format-options '{\"snippetInterface\": \"synchronous\" }' --dry-run --no-strict ",
        "update-package-json-dependencies": "npx ncu -u && npm i",
        "install-chromedriver": "selenium-standalone install --drivers.chrome.version=83.0.4103.39 --drivers.chrome.baseURL=https://chromedriver.storage.googleapis.com --drivers.firefox.version=0.26.0 --drivers.ie.version=0.26.0 --drivers.ie.baseURL=http://selenium-release.storage.googleapis.com",
        "test:report": "allure serve ./report/allure-results",
        "clean": "read -p \"Are you sure you want to clean the project? [y/n] \" REPLY; if [[ $REPLY =~ ^[Yy]$ ]]; then rm -R .git .github demo-app test .codeclimate.yml .travis.yml jest.json wdio.BUILD.conf.js src/features/**; fi",
        "test:lint": "eslint 'src/**/*.js' 'test/**/*.js!(setup.js)'",
        "generate:eslintrc": "npx tslint-to-eslint-config",
        "eslint-fix-all": "npx eslint test/**/*.ts --fix",
        "lint": "tsc --noEmit && eslint '*/**/*.{js,ts,tsx}' --quiet --fix"
    },
    "devDependencies": {
        "@cucumber/cucumber": "7.2.1",
        "@types/chai": "4.2.18",
        "@types/chai-as-promised": "7.1.4",
        "@types/fs-extra": "9.0.11",
        "@types/jquery": "3.5.5",
        "@types/long": "4.0.1",
        "@types/node": "15.6.2",
        "@types/reflect-metadata": "0.1.0",
        "@types/yargs": "16.0.3",
        "@typescript-eslint/eslint-plugin": "4.26.0",
        "@typescript-eslint/eslint-plugin-tslint": "4.26.0",
        "@typescript-eslint/parser": "4.26.0",
        "@typescript-eslint/typescript-estree": "4.26.0",
        "@wdio/cli": "7.7.2",
        "@wdio/config": "7.7.0",
        "@wdio/cucumber-framework": "7.7.2",
        "@wdio/dot-reporter": "7.7.0",
        "@wdio/local-runner": "7.7.2",
        "@wdio/mocha-framework": "7.7.2",
        "@wdio/protocols": "^7.5.3",
        "@wdio/selenium-standalone-service": "7.7.0",
        "@wdio/spec-reporter": "7.7.0",
        "@wdio/sync": "7.7.2",
        "@wdio/types": "7.7.0",
        "axios": "0.21.1",
        "chai": "4.3.4",
        "chai-as-promised": "7.1.1",
        "cucumber-messages": "8.0.0",
        "dotenv": "10.0.0",
        "eslint": "7.27.0",
        "eslint-config-airbnb-base": "14.2.1",
        "eslint-config-prettier": "8.3.0",
        "eslint-config-standard": "16.0.3",
        "eslint-plugin-import": "2.23.4",
        "eslint-plugin-jsdoc": "35.1.2",
        "eslint-plugin-node": "11.1.0",
        "eslint-plugin-prefer-arrow": "1.2.3",
        "eslint-plugin-prettier": "3.4.0",
        "eslint-plugin-promise": "5.1.0",
        "eslint-plugin-standard": "5.0.0",
        "eslint-plugin-wdio": "7.4.2",
        "exceljs": "4.2.1",
        "forever": "4.0.0",
        "fs-extra": "10.0.0",
        "http-server": "0.12.3",
        "husky": "6.0.0",
        "import": "0.0.6",
        "lint-staged": "11.0.0",
        "merge": "2.1.1",
        "npm-check-updates": "11.6.0",
        "npm-run-all": "4.1.5",
        "nyc": "15.1.0",
        "prettier": "2.3.0",
        "reflect-metadata": "^0.1.13",
        "save-dev": "0.0.1-security",
        "stylelint": "13.13.1",
